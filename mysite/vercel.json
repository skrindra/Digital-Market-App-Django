{
    "version":2,
    "build": [
        {
            "src":"mysite/wsgi.py",
            "use":"@vercel/python",
            "config":{ "maxLabdaSize": "15mb","runtime": "python3.11"}    
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{"distDir":"staticfiles_build"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "mysite/mysite/wsgi.py"
        }
    ]

}